<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="model.Exercise" table="EXERCISE" catalog="gymtonik_database">
        <id name="idExercice" type="int" column="idExercise">
            <generator class="native"/>
        </id>
        <property name="name" type="string" not-null="true" />
        <property name="explanation" type="string" not-null="true" />
        <property name="length" type="int"/>
        <list name="bodyPart" inverse="false" table="MUSCLE">
            <key>
                <column name="id" />
            </key>
            <index>
            </index>
            <element type="string">
                <column name="refBodyPart" />
            </element>
        </list>
    </class>
</hibernate-mapping>