<?xml version="1.0"?><!DOCTYPE hibernate-mapping
        PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>
    <class name="model.Session" table="SESSION">
        <id name="id" type="int" column="idSession">
            <generator class="native"/>
        </id>
        <property name="timeSleep" type="int"/>
        <property name="perform" type="boolean"/>
        <property name="dateProgram" type="timestamp"/>
        <list name="trainings" inverse="false" table="EXERCISE" lazy="true">
            <list-index>
            </list-index>
            <key>
                <column name="id" />
            </key>
            <one-to-many class="model.Exercise"/>
            <element type="java.lang.String">
                <column name="refTrainings" />
            </element>
        </list>
    </class>
</hibernate-mapping>